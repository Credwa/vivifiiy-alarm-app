{
  "expo": {
    "name": "vivifiiy-alarm-app",
    "slug": "vivifiiy-alarm-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "vivifiiyalarmapp",
    "primaryColor": "#100F62",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#100F62"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
