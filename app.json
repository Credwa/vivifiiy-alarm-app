      "buildNumber": "1.0"
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.credwa.vivifiiyalarmapp",
      "versionCode": 1,
      "permissions": []
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
